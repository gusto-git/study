<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="d" class="com.gusto.sdi5261.dog.Dog"></bean>
	<bean id="d2" class="com.gusto.sdi5261.dog.Dog">
		<property name="name" value="멍멍이"></property>
		<property name="age" value="2"></property>
	</bean>
	
	<bean id="b" class="com.gusto.sdi5261.book.Book">
		<property name="name" value="12가지 인생의법칙"></property>
		<property name="price" value="6500"></property>
	</bean>
	
	<bean id="b2" class="com.gusto.sdi5261.book.Book">
		<constructor-arg value="세이노의 가르침"></constructor-arg>
		<constructor-arg value="6480"></constructor-arg>
	</bean>
	
</beans>
